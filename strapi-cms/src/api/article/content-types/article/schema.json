{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": "SEO optimized articles generated by SEOAgent"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "excerpt": {
      "type": "text",
      "maxLength": 500
    },
    "metaTitle": {
      "type": "string",
      "maxLength": 60
    },
    "metaDescription": {
      "type": "text",
      "maxLength": 160
    },
    "keywords": {
      "type": "json"
    },
    "primaryKeyword": {
      "type": "string"
    },
    "wordCount": {
      "type": "integer"
    },
    "readingTime": {
      "type": "integer"
    },
    "seoScore": {
      "type": "integer",
      "min": 0,
      "max": 100
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "articles"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "articles"
    },
    "author": {
      "type": "string",
      "default": "SEOAgent"
    },
    "publishedAt": {
      "type": "datetime"
    },
    "seoAgentData": {
      "type": "json"
    },
    "publicUrl": {
      "type": "string"
    },
    "externalId": {
      "type": "string"
    }
  }
}