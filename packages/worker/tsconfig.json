{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "outDir": "dist",
    "rootDir": "../../",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2022",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "../../"
  },
  "include": [
    "src/**/*.ts",
    "../../src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "../../strapi-cms/**/*",
    "../../supabase/functions/**/*"
  ]
}
